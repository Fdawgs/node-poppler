{
	"name": "node-poppler",
	"version": "1.7.1",
	"description": "Node.js wrapper for the Poppler PDF rendering library",
	"keywords": [
		"poppler",
		"pdf",
		"pdf-converter",
		"pdf-to-html"
	],
	"main": "src/index.js",
	"repository": "git+https://github.com/Fdawgs/node-poppler.git",
	"author": "Frazer Smith",
	"license": "MIT",
	"engines": {
		"node": ">=12.16.3"
	},
	"scripts": {
		"changelog": "conventional-changelog -p @commitlint/config-conventional -s -i CHANGELOG.md && prettier CHANGELOG.md --write",
		"doc": "jsdoc2md src/index.js > API.md && prettier API.md --write",
		"dupe-check": "yarn jsinspect ./src",
		"jest": "jest src --coverage",
		"lint": "eslint . --cache --ext js,jsx,ts,tsx",
		"prettier": "prettier . --write --ignore-path .gitignore",
		"test": "yarn lint && yarn test-only",
		"test-coverage": "cross-env NODE_ENV=test jest --coverage --runInBand",
		"test-only": "cross-env NODE_ENV=test jest --detectOpenHandles --runInBand"
	},
	"jest": {
		"clearMocks": true,
		"testEnvironment": "node",
		"testTimeout": 10000
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^8.3.4",
		"conventional-changelog-cli": "^2.0.34",
		"coveralls": "^3.1.0",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.21.1",
		"eslint-plugin-jest": "^23.13.2",
		"eslint-plugin-jsdoc": "^26.0.0",
		"eslint-plugin-json": "^2.1.1",
		"eslint-plugin-promise": "^4.2.1",
		"glob": "^7.1.6",
		"jest": "^26.0.1",
		"jsdoc-to-markdown": "^5.0.3",
		"jsinspect": "^0.12.7",
		"prettier": "2.0.5",
		"typescript": "^3.9.3"
	},
	"dependencies": {}
}
